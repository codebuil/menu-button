	public void  onmenu1_click(View v) {
		setTitle("menu1");
		System.setProperty("menu1","1");

					try{
						Intent intent = new Intent(context,MainActivity1.class);

						startActivity(intent);  

						finish(); 
					}catch(Exception e){}

				
	} 
